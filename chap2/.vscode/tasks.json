{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "type": "shell",
            "command": "gcc",
            "args": [
                "mmult.c",
                "fastTranspose.c",
                "-o",
                "mmult",
                "-g"
            ],
            "group": "build",
            "problemMatcher": [
                "$gcc"
            ]
        },
        {
            "label": "build skip",
            "type": "shell",
            "command": "gcc",
            "args": [
                "skipsearch.c",
                "-o",
                "skipsearch",
                "-g"
            ],
            "group": "build",
            "problemMatcher": [
                "$gcc"
            ]
        },
    ]
}